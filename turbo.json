{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "db:generate": {
      "inputs": ["prisma/schema.prisma"],
      "outputs": ["../../node_modules/.prisma/client"],
      "cache": false
    },
    "db:push": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "outputs": [".next/**", ".expo/**"]
    },
    "lint": {},
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "type-check": {
      "dependsOn": ["^db:generate"],
      "cache": false
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "NODE_ENV",
    "FACEBOOK_ID",
    "FACEBOOK_SECRET",
    "GOOGLE_ID",
    "GOOGLE_SECRET",
    "STRAVA_CLIENT_ID",
    "STRAVA_CLIENT_SECRET",
    "EMAIL_FROM",
    "EMAIL_SERVER_USER",
    "EMAIL_SERVER_PASSWORD",
    "EMAIL_SERVER_HOST",
    "EMAIL_SERVER_PORT",
    "AWS_ACCESS_KEY_ID_WSC",
    "AWS_SECRET_ACCESS_KEY_WSC",
    "STREAMCHAT_API_KEY",
    "STREAMCHAT_SECRET_KEY",
    "NEXT_PUBLIC_MAPBOX_TOKEN",
    "NEXT_PUBLIC_MAPQUEST_KEY",
    "NEXT_PUBLIC_STREAMCHAT_API_KEY"
  ]
}
